    <description>AD-AD NOKIASC-25549: Process Builder for Address Object.</description>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>IF (
     ISBLANK ( [Address].Street ) , &quot;&quot; , [Address].Street + &quot;, &quot; )
     + IF (
             ISBLANK ( [Address].PostalCode ) , &quot;&quot; , [Address].PostalCode + &quot; - &quot; )
             + IF ( 
                     ISBLANK ( [Address].City) , &quot;&quot; , [Address].City + &quot;, &quot; )
                     + IF ( 
                             ISBLANK ( [Address].State ) , &quot;&quot; , [Address].State  + &quot; - &quot; )
                              + IF ( 
                                      ISBLANK ( [Address].Country ) , &quot;&quot; , [Address].Country )</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_8_myRule_7_A1_9403680954</name>
        <dataType>String</dataType>
        <expression>IF (
     ISBLANK ( {!myVariable_current.Street} ) , &quot;&quot; , {!myVariable_current.Street} + &quot;, &quot; )
     + IF (
             ISBLANK ( {!myVariable_current.PostalCode} ) , &quot;&quot; , {!myVariable_current.PostalCode} + &quot; - &quot; )
             + IF ( 
                     ISBLANK ( {!myVariable_current.City}) , &quot;&quot; , {!myVariable_current.City} + &quot;, &quot; )
                     + IF ( 
                             ISBLANK ( {!myVariable_current.State} ) , &quot;&quot; , {!myVariable_current.State}  + &quot; - &quot; )
                              + IF ( 
                                      ISBLANK ( {!myVariable_current.Country} ) , &quot;&quot; , {!myVariable_current.Country} )</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>OR(
(OR(CONTAINS($Setup.Global_Control__c.Other_ProcessFlow__c, &quot;AD-AD&quot;), 
CONTAINS($Setup.Global_Control__c.Other_ProcessFlow__c, &quot;*&quot;), ($Setup.Global_Control__c.All_ProcessFlow__c)) &amp;&amp; $Setup.Global_Control__c.Disabled__c))
</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
        <expression>OR(
(OR(CONTAINS({!$Setup.Global_Control__c.Other_ProcessFlow__c}, &quot;AD-AD&quot;), 
CONTAINS({!$Setup.Global_Control__c.Other_ProcessFlow__c}, &quot;*&quot;), ({!$Setup.Global_Control__c.All_ProcessFlow__c})) &amp;&amp; {!$Setup.Global_Control__c.Disabled__c}))</expression>
    </formulas>
    <formulas>
