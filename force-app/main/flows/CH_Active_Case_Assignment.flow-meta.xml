    <description>A notification is sent when a case is assigned</description>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>(NOT(ISPICKVAL([Case].Status , &apos;Closed&apos;) || ISPICKVAL([Case].Status , &apos;Cancelled&apos;)))
&amp;&amp; 
(ISPICKVAL([Case].CH_Outage__c , &apos;No&apos;) || NOT(ISPICKVAL([Case].CH_Outage__c , &apos;NULL&apos;) ))  
&amp;&amp; 
(([Case].RecordType.DeveloperName = &apos;CH_NokiaStandardCase&apos;) || ([Case].RecordType.DeveloperName =  &apos;CH_NokiaInformationRequest&apos;) || ([Case].RecordType.DeveloperName =  &apos;CH_Problem&apos;)) 
&amp;&amp; 
(ISNEW() || 
ISCHANGED([Case].OwnerId)
)
&amp;&amp;(BEGINS([Case].OwnerId , &quot;005&quot;))
&amp;&amp; NOT(BEGINS([Case].Owner:User.Username , &quot;virtual_portal_attendant@&quot;))</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
