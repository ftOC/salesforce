&amp;&amp; 
(({!myVariable_current.RecordType.DeveloperName} = &apos;CH_NokiaStandardCase&apos;) || ({!myVariable_current.RecordType.DeveloperName} =  &apos;CH_NokiaInformationRequest&apos;) || ({!myVariable_current.RecordType.DeveloperName} =  &apos;CH_Problem&apos;)) 
