    </formulas>
    <formulas>
        <name>RemoveFirst</name>
        <dataType>String</dataType>
        <expression>TRIM(LEFT({!Alphabetuse}, FIND(&quot;,&quot;, {!Alphabetuse})-1))</expression>
    </formulas>
    <formulas>
        <name>Sites</name>
        <dataType>String</dataType>
