    </formulas>
    <formulas>
        <name>SplitAlphabet</name>
        <dataType>String</dataType>
        <expression>TRIM(RIGHT({!Alphabetuse}, LEN({!Alphabetuse}) - FIND(&quot;,&quot;, {!Alphabetuse})))</expression>
    </formulas>
