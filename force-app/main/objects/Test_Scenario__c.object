    <fields>
        <fullName>E2E_UAT_Overall_Priority__c</fullName>
        <externalId>false</externalId>
        <formula>IF(of_E2E_P1_Scripts__c  &gt; 0, &quot;1&quot;, 
  IF(AND(of_E2E_P2_Scripts__c &gt; 0, of_E2E_P1_Scripts__c = 0), &quot;2&quot;, 
  IF(AND(of_E2E_P2_Scripts__c = 0, of_E2E_P1_Scripts__c = 0, of_E2E_P3_Scripts__c &gt; 0), &quot;3&quot;,
  &quot;Other&quot;
)
)
)</formula>
        <inlineHelpText>1 = at least one related Test Script is Priority 1
2 = at least one related Test Script is Priority 2 and no Priority 1s
Other = no related Priority 1 or 2 Test Scripts</inlineHelpText>
        <label>E2E UAT Overall Priority</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>E2E_UAT_Overall_Status__c</fullName>
        <externalId>false</externalId>
        <formula>IF(of_Test_E2E_Scripts_Not_Started__c = of_Test_E2E_Scripts__c, &quot;Not Started&quot;, 
  IF(of_Test_E2E_Scripts_Not_Applicable__c = of_Test_E2E_Scripts__c, &quot;Not Applicable&quot;, 
  IF(Has_50_Pass_Rate__c = TRUE, &quot;Pass&quot;,
  IF(of_IT_Passed_E2E_Test_Scripts__c &gt; 0, &quot;Pass&quot;,
  IF(AND(of_Test_E2E_Scripts_Pass__c &gt; 0, of_Test_E2E_Scripts_Fail__c = 0, of_Test_E2E_Scripts_Script_Error__c = 0), &quot;Pass&quot;,
  IF(OR(of_Test_E2E_Scripts_Fail__c &gt; 0, of_Test_E2E_Scripts_Script_Error__c &gt; 0), &quot;Fail&quot;,


  &quot;In Progress&quot;
)
)
)
)
)
)</formula>
        <inlineHelpText>Not Started = all scripts Not Started
Pass = at least one Pass and no Fail or Script Error
Fail = at least one Fail or Script Error</inlineHelpText>
        <label>E2E UAT Overall Status</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
