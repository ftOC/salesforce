     Team__c = &quot;Interfaces/ETL&quot;,
     Team__c = &quot;Approvals/IWA&quot;,
     Team__c = &quot;Mobile&quot;,
     Team__c = &quot;Security&quot;,
     Team__c = &quot;Data Migration&quot;
  ), &quot;Unanimous&quot;,
  IF(
    OR(Team__c = &quot;Sales Process&quot;,
       Team__c = &quot;Doc Strategy&quot;,
       Team__c = &quot;LoA&quot;,
