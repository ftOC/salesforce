    <validationRules>
        <fullName>Email_Notification_Validation_Check</fullName>
        <active>true</active>
        <description>Req#3574</description>
        <errorConditionFormula>AND(NOT(OR( 
$Profile.Name = &apos;Data Loader Profile&apos; ,
$Profile.Name = &apos;System Administrator&apos;
)),
ISCHANGED(Enrollment_Notification__c ),
Enrollment_Notification__c  = false)</errorConditionFormula>
        <errorDisplayField>Enrollment_Notification__c</errorDisplayField>
        <errorMessage>You are only allowed to send the initial enrollment notification once.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_date_should_have_Greater_value</fullName>
        <active>true</active>
        <errorConditionFormula>Start_Date__c  &gt;=   End_Date__c</errorConditionFormula>
        <errorDisplayField>End_Date__c</errorDisplayField>
        <errorMessage>Rebate End Date cannot be less than the Rebate Start Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EnrollmenEnd_date_should_have_Greater_v</fullName>
        <active>true</active>
        <description>D-4346</description>
        <errorConditionFormula>Enrollment_start_date__c  &gt;=  Enrollment_End_Date__c</errorConditionFormula>
        <errorDisplayField>Enrollment_End_Date__c</errorDisplayField>
        <errorMessage>Enrollment End Date cannot be less than the Enrollment Start Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Enrollment_End_date_will_be_a_current_or</fullName>
        <active>true</active>
        <errorConditionFormula>Enrollment_End_Date__c &lt;  TODAY()</errorConditionFormula>
        <errorDisplayField>Enrollment_End_Date__c</errorDisplayField>
        <errorMessage>Enrollment End date will be a current or future date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Rebate_Start_Date_can_be_date_in_the_pas</fullName>
        <active>true</active>
        <description>D-4285</description>
        <errorConditionFormula>Start_Date__c  &lt;  TODAY()</errorConditionFormula>
        <errorDisplayField>Start_Date__c</errorDisplayField>
        <errorMessage>Rebate Start date, Only current and future dates could be entered.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RestrictNotifBeforeEnrollmentStart</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISCHANGED(Enrollment_Notification__c),
Enrollment_Notification__c,
Enrollment_start_date__c &gt; Today())</errorConditionFormula>
        <errorDisplayField>Enrollment_Notification__c</errorDisplayField>
        <errorMessage>Enrollment has not yet started for this rebate period. Please send enrollment notification on &quot;Enrollment Start Date&quot;</errorMessage>
    </validationRules>
