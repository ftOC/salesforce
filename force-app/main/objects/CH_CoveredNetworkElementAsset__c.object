    <validationRules>
        <fullName>CH_CDBContractTypeInternal</fullName>
        <active>true</active>
        <errorConditionFormula>CONTAINS(CH_ContractLineItem__r.ServiceContract.CH_CDB_ContractType__c, &apos;Internal&apos;)</errorConditionFormula>
        <errorMessage>This Contract Line Item cannot be selected. The service contract cannot have CDB Contract Type of Internal.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CH_ValidateCLIServiceClassificationOnNEA</fullName>
        <active>true</active>
        <description>NOKIASC-24906</description>
        <errorConditionFormula>ISBLANK(CH_ContractLineItem__r.CH_ServiceClassification__c )</errorConditionFormula>
        <errorMessage>Service Classification on Contract Line Item is null. So, cant associate Network Element Asset to CLI</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CH_ValidationForCLIandNEA</fullName>
        <active>true</active>
        <description>NOKIASC-24277</description>
        <errorConditionFormula>AND(
    CH_ContractLineItem__r.CH_ServiceClassification__c !=&quot;HWS Service&quot; ,
    (CH_NetworkElementAsset__r.CH_CountryISOName__c &lt;&gt; CH_ContractLineItem__r.Asset.CH_CountryISOName__c) || 
    (CH_NetworkElementAsset__r.Product2.Name &lt;&gt;  CH_ContractLineItem__r.Asset.Product2.Name) || 
    ((CH_NetworkElementAsset__r.CH_ProductVariant__c  &lt;&gt;  CH_ContractLineItem__r.Asset.CH_ProductVariant__c) &amp;&amp; (CH_ContractLineItem__r.Asset.CH_ProductVariant__c  &lt;&gt; &apos;&apos;) &amp;&amp; (CH_NetworkElementAsset__r.CH_ProductVariant__c  &lt;&gt; &apos;&apos;)) || 
    ((CH_NetworkElementAsset__r.CH_Solution__c  &lt;&gt;  CH_ContractLineItem__r.Asset.CH_Solution__c) &amp;&amp; (CH_ContractLineItem__r.Asset.CH_Solution__c  &lt;&gt; &apos;&apos;) &amp;&amp; (CH_NetworkElementAsset__r.CH_Solution__c  &lt;&gt; &apos;&apos;)) || 
    ((CH_NetworkElementAsset__r.CH_ProductRelease__c  &lt;&gt;  CH_ContractLineItem__r.Asset.CH_ProductRelease__c) &amp;&amp; (CH_NetworkElementAsset__r.CH_ProductRelease__c  &lt;&gt; &apos;&apos;))
)</errorConditionFormula>
        <errorMessage>CLI and NE Asset should have the same Country, Product, Product Variant, Solution and Product Release. or Product Release field can be blank on NE Asset</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CH_ValidationForCLIandNEAForHWS</fullName>
        <active>true</active>
        <description>NOKIASC-24916 - For HWS</description>
        <errorConditionFormula>AND( 
	CH_ContractLineItem__r.CH_ServiceClassification__c =&quot;HWS Service&quot; ,
	 OR (
			CH_ContractLineItem__r.ServiceContract.AccountId &lt;&gt;  CH_NetworkElementAsset__r.AccountId ,
			CH_NetworkElementAsset__r.CH_CountryISOName__c &lt;&gt; CH_ContractLineItem__r.Asset.CH_CountryISOName__c ,
			CH_NetworkElementAsset__r.Product2Id &lt;&gt;  CH_ContractLineItem__r.Asset.Product2Id
		)
	)</errorConditionFormula>
        <errorMessage>CLI and NE Asset should have the same Country, Product and Product Release. or Product Release field can be blank on NE Asset</errorMessage>
    </validationRules>
