    <validationRules>
        <fullName>CH_3PVendor_ReadonlyOriginType</fullName>
        <active>true</active>
        <description>The &quot;Origin&quot; and &quot;TYpe&quot;  field will be read only while editing.</description>
        <errorConditionFormula>OR(
				ISCHANGED(CH_Origin__c),
				ISCHANGED(CH_Type__c)
			)</errorConditionFormula>
        <errorMessage>Fields Type/Origin  cannot be modified.</errorMessage>
    </validationRules>
