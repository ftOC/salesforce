<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Cannot_remove_stakeholder</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
NOT(ISBLANK(PRIORVALUE(Participant__c))),
ISCHANGED(Participant__c),
NOT(ISNEW())
)</errorConditionFormula>
    <errorDisplayField>Participant__c</errorDisplayField>
    <errorMessage>Cannot change Participant field.</errorMessage>
</ValidationRule>
