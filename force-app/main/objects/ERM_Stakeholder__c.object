    <validationRules>
        <fullName>CannotUpdateEventOnStakeholder</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT(ISBLANK(PRIORVALUE(Event__c))),
ISCHANGED(Event__c),
NOT(ISNEW())
)</errorConditionFormula>
        <errorDisplayField>Event__c</errorDisplayField>
        <errorMessage>You cannot update the Event of this Stakeholder.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_remove_stakeholder</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT(ISBLANK(PRIORVALUE(Participant__c))),
ISCHANGED(Participant__c),
NOT(ISNEW())
)</errorConditionFormula>
        <errorDisplayField>Participant__c</errorDisplayField>
        <errorMessage>Cannot change Participant field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Content_Stakehoder_cant_answer_events</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
				 Event__r.RecordType.DeveloperName = &quot;ERM&quot;
   , OR(
								 ISPICKVAL(Role__c, &quot;Content Owner&quot;)
       , ISPICKVAL(Role__c, &quot;Content Support&quot;) 
				 )
			, OR(
								 ISPICKVAL(Status__c, &quot;Confirmed&quot;)
       , ISPICKVAL(Status__c, &quot;Rejected&quot;)
					)
)</errorConditionFormula>
        <errorMessage>Only a speaker or assistant can accept or reject an invitation for ERM.</errorMessage>
    </validationRules>
