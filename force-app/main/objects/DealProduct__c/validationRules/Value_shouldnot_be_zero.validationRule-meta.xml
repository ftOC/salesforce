<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Value_shouldnot_be_zero</fullName>
    <active>true</active>
    <errorConditionFormula>(($Profile.Name &lt;&gt; $Label.Data_Loader_Profile_Name) &amp;&amp; (Value__c &lt;= 0 || ISBLANK(Value__c)))
 &amp;&amp; (($Profile.Name &lt;&gt; $Label.System_Admin_Profile) &amp;&amp; (Value__c &lt;= 0 || ISBLANK(Value__c)))</errorConditionFormula>
    <errorDisplayField>Value__c</errorDisplayField>
    <errorMessage>Line Item Value should be greater than 0 and cannot be blank.</errorMessage>
</ValidationRule>
