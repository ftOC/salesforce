<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Reason_for_override_is_required</fullName>
    <active>true</active>
    <errorConditionFormula>AND(  NOT(ISPICKVAL( Rebate_Status_Override__c , &apos;&apos;))  ,  ISPICKVAL( Reason_for_Override__c , &apos;&apos;)  )</errorConditionFormula>
    <errorMessage>Please enter Reason for Override</errorMessage>
</ValidationRule>
