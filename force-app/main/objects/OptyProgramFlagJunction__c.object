    <fields>
        <fullName>Contributing_Amount__c</fullName>
        <description>Contributing Amount = PF% x Opportunity Unweighted Value EUR. Requirement 5791.</description>
        <externalId>false</externalId>
        <formula>Percentage__c  * Opportunity__r.Unweighted_Amount_EUR__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Contributing Amount</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Description__c</fullName>
        <description>This field will capture description of related Program Flag Data record</description>
        <externalId>false</externalId>
        <formula>Program_Flag__r.Description__c</formula>
        <label>Description</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Flag_Category__c</fullName>
        <description>This field will capture Flag Category of the related Program Flag Data record</description>
        <externalId>false</externalId>
        <formula>TEXT( Program_Flag__r.Flag_Category__c )</formula>
        <label>Flag Category</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Is_Flag_Active__c</fullName>
        <description>(from PFD object field)</description>
        <externalId>false</externalId>
        <formula>Program_Flag__r.Is_Flag_Active__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>If flag is inactive, it can&apos;t be selected in an opportunity</inlineHelpText>
        <label>Is Flag Active</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Opportunity__c</fullName>
        <description>Lookup Relationship with Opportunity</description>
        <externalId>false</externalId>
        <label>Opportunity</label>
        <referenceTo>Opportunity</referenceTo>
        <relationshipLabel>Program Flags</relationshipLabel>
        <relationshipName>Program_Flags</relationshipName>
        <relationshipOrder>0</relationshipOrder>
        <reparentableMasterDetail>false</reparentableMasterDetail>
        <trackTrending>false</trackTrending>
        <type>MasterDetail</type>
        <writeRequiresMasterRead>false</writeRequiresMasterRead>
    </fields>
    <fields>
        <fullName>Percentage__c</fullName>
        <defaultValue>1</defaultValue>
        <description>Percentage the opportunity is associated to a Program Flag. Requirement 5791.</description>
        <externalId>false</externalId>
        <inlineHelpText>Percentage of the Opportunity Unweighted Value contributing to this Program Flag</inlineHelpText>
        <label>Percentage</label>
        <precision>3</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Percent</type>
    </fields>
    <fields>
        <fullName>Program_Flag__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <externalId>false</externalId>
        <label>Program Flag</label>
        <lookupFilter>
            <active>true</active>
            <filterItems>
                <field>Program_Flag__c.Is_Flag_Active__c</field>
                <operation>equals</operation>
                <value>True</value>
            </filterItems>
            <isOptional>false</isOptional>
        </lookupFilter>
        <referenceTo>Program_Flag__c</referenceTo>
        <relationshipLabel>Program Flags</relationshipLabel>
        <relationshipName>Program_Flags</relationshipName>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <label>Program Flag</label>
    <nameField>
