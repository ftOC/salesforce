<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Pricing_override_expiration_mandatory</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
	NOT(ISBLANK(Text(Pricing_Level_Override__c))),
	ISBLANK(Pricing_Override_Expiration__c),
        NOT(ISPICKVAL(Pricing_Level_Override__c, &apos;No Accreditation&apos;))

)</errorConditionFormula>
    <errorDisplayField>Pricing_Override_Expiration__c</errorDisplayField>
    <errorMessage>Please add a Pricing Override Expiration Date.</errorMessage>
</ValidationRule>
