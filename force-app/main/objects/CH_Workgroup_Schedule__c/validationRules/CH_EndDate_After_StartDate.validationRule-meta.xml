<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CH_EndDate_After_StartDate</fullName>
    <active>true</active>
    <errorConditionFormula>DATETIMEVALUE(CH_End_DateTime__c) &lt;=  DATETIMEVALUE(CH_Start_DateTime__c)</errorConditionFormula>
    <errorMessage>The end date needs to be later then the start date</errorMessage>
</ValidationRule>
