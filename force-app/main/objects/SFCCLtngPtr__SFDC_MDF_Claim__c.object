    <validationRules>
        <fullName>SFCCLtngPtr__Amount_Limits</fullName>
        <active>true</active>
        <description>The amount must be greater than zero</description>
        <errorConditionFormula>SFCCLtngPtr__Amount__c &lt;= 0</errorConditionFormula>
        <errorDisplayField>SFCCLtngPtr__Amount__c</errorDisplayField>
        <errorMessage>The claim amount must be greater than zero</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SFCCLtngPtr__Require_Other_Expenditure_Type</fullName>
        <active>true</active>
        <description>Used to force the user to choose fill in the &apos;other expenditure type&apos; when &apos;other&apos; is chosen as the expenditure type</description>
        <errorConditionFormula>AND(ISPICKVAL( SFCCLtngPtr__Expenditure_Type__c, &quot;Other&quot;), LEN(SFCCLtngPtr__Other_Expenditure_Type__c )=0)</errorConditionFormula>
        <errorDisplayField>SFCCLtngPtr__Other_Expenditure_Type__c</errorDisplayField>
        <errorMessage>Please provide details on the &apos;Other&apos; expenditure type.</errorMessage>
    </validationRules>
