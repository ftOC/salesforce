    <validationRules>
        <fullName>BlockApprovalCommentAfterOnWeek</fullName>
        <active>false</active>
        <errorConditionFormula>AND(PRIORVALUE( Apttus_Approval__Approver_Comments__c )&lt;&gt; Apttus_Approval__Approver_Comments__c, 
(NOW() - Tech_Approval_Date__c )*24 &gt;= 7 , 
NF_Assignee_Type__c =&apos;Validator&apos;, 
!ISBLANK(Tech_Status__c ))</errorConditionFormula>
        <errorMessage>You can not leave validation comments after one week of approval decision</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NF_BlockApprovalCommentAfterOnWeek</fullName>
        <active>true</active>
        <errorConditionFormula>AND(PRIORVALUE( Apttus_Approval__Approver_Comments__c )&lt;&gt; Apttus_Approval__Approver_Comments__c,
NOW() - Tech_Approval_Date__c &gt;= 7 ,
NF_Assignee_Type__c =&apos;Validator&apos; ,
!ISBLANK(Tech_Status__c ) )</errorConditionFormula>
        <errorMessage>You can not leave validation comments after one week of approval decision</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NF_FreezeApprovalRequBasedOnSalesOutcome</fullName>
        <active>true</active>
        <description>You can not change the Approval request record if the Sales Outcome is &apos;Cancelled by Customer&apos;, &apos;Lost to Competitor&apos;, &apos;Obsolete&apos; or &apos;Withdrawn by Nokia&apos;.</description>
        <errorConditionFormula>AND( 
OR( 
ISPICKVAL(Apttus_Approval__Related_Opportunity__r.StageName, &apos;Closed - Cancelled by Customer&apos;), 
ISPICKVAL(Apttus_Approval__Related_Opportunity__r.StageName, &apos;Closed - Lost to Competitor&apos;), 
ISPICKVAL(Apttus_Approval__Related_Opportunity__r.StageName, &apos;Closed – Obsolete&apos;), 
ISPICKVAL(Apttus_Approval__Related_Opportunity__r.StageName, &apos;Closed - Withdrawn by Nokia&apos;) 
), 
$Profile.Name&lt;&gt;&apos;System Administrator&apos;, 
$Profile.Name&lt;&gt;&apos;Global Process Admin Profile&apos;, 
$Profile.Name&lt;&gt;&apos;Integration API Only Profile&apos;, 
$Profile.Name&lt;&gt;&apos;Data Loader Profile&apos; 
)</errorConditionFormula>
        <errorMessage>You cannot change the Approval Request record if the Opportunity Sales Phase is &apos;Closed - Cancelled by Customer&apos; or &apos;Closed - Lost to Competitor&apos; or &apos;Closed - Obsolete&apos; or &apos;Closed - Withdrawn by Nokia&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NF_Validation_Status_Check</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 

$Profile.Name &lt;&gt; $Label.Data_Loader_Profile_Name, 
ISCHANGED(Validator_Status__c), 
OR( 
ISPICKVAL(PRIORVALUE(Validator_Status__c),&apos;Validated&apos;), 
ISPICKVAL(PRIORVALUE(Validator_Status__c),&apos;Rejected&apos;) 
), 
OR( 
ISPICKVAL((Validator_Status__c),&apos;Validated&apos;), 
ISPICKVAL((Validator_Status__c),&apos;Rejected&apos;), 
ISPICKVAL((Validator_Status__c),&apos;&apos;) 

) 

)</errorConditionFormula>
        <errorMessage>Validation Request Already Completed</errorMessage>
    </validationRules>
