    <fields>
        <fullName>CH_Account__c</fullName>
        <description>Account Lookup</description>
        <externalId>false</externalId>
        <label>Account</label>
        <lookupFilter>
            <active>true</active>
            <filterItems>
                <field>Account.RecordTypeId</field>
                <operation>equals</operation>
                <value>Legal Entity</value>
            </filterItems>
            <isOptional>false</isOptional>
        </lookupFilter>
        <referenceTo>Account</referenceTo>
        <relationshipLabel>Authorized Account</relationshipLabel>
        <relationshipName>Authorized_Account</relationshipName>
        <relationshipOrder>0</relationshipOrder>
        <reparentableMasterDetail>true</reparentableMasterDetail>
        <trackTrending>false</trackTrending>
        <type>MasterDetail</type>
        <writeRequiresMasterRead>true</writeRequiresMasterRead>
    </fields>
    <fields>
        <fullName>CH_EntitlementId__c</fullName>
        <externalId>false</externalId>
        <formula>CH_ServiceContract__r.CH_Entitlement__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Entitlement ID</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>CH_FlagRequired__c</fullName>
        <defaultValue>true</defaultValue>
        <description>Flag Required defeult to Y</description>
        <externalId>false</externalId>
        <label>Flag Required</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>CH_IsTriggerRun__c</fullName>
        <defaultValue>true</defaultValue>
        <description>Flag used to identify whether trigger should run authorized contact functionality or not</description>
        <externalId>false</externalId>
        <label>Is Trigger Run</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>CH_ServiceContract__c</fullName>
        <description>Service Contract Lookup</description>
        <externalId>false</externalId>
        <label>Service Contract</label>
        <referenceTo>ServiceContract</referenceTo>
        <relationshipLabel>Authorized Account</relationshipLabel>
        <relationshipName>Authorized_Account</relationshipName>
        <relationshipOrder>1</relationshipOrder>
        <reparentableMasterDetail>false</reparentableMasterDetail>
        <trackTrending>false</trackTrending>
        <type>MasterDetail</type>
        <writeRequiresMasterRead>false</writeRequiresMasterRead>
    </fields>
    <label>Authorized Account</label>
    <nameField>
        <displayFormat>{0000000}</displayFormat>
        <label>Authorized Contact</label>
        <type>AutoNumber</type>
    </nameField>
    <pluralLabel>Authorized Account</pluralLabel>
    <searchLayouts/>
    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>CH_ValidAccountName</fullName>
        <active>true</active>
        <description>If the Account Name contains &quot;Nokia OYJ&quot; then it is valid account name else if contains &quot;Nokia&quot; the it is invalid</description>
        <errorConditionFormula>CONTAINS(LOWER(CH_Account__r.Name), LOWER($Setup.CH_AccountName__c.CH_InvalidName__c))</errorConditionFormula>
        <errorDisplayField>CH_Account__c</errorDisplayField>
        <errorMessage>The selected account is not valid for authorization.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
