        <fullName>CH_Target_Date__c</fullName>
        <externalId>false</externalId>
        <formula>IF  (   ISPICKVAL(CH_Priority__c , &quot;Emergency&quot;) , CreatedDate + (7* 4), 
			   
					   IF(ISPICKVAL(CH_Priority__c , &quot;High&quot;) , CreatedDate + (7* 8), 
											
											IF(ISPICKVAL(CH_Priority__c , &quot;Medium&quot;) , CreatedDate + (7* 12), null)
									) 
			
				
	)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
