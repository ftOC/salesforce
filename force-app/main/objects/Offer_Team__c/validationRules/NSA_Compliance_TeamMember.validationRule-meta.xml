<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>NSA_Compliance_TeamMember</fullName>
    <active>true</active>
    <description>Req# 3353-Enhancement Request - Ability to block oppty team add&apos;s missing NSA compliance flag</description>
    <errorConditionFormula>IF( 
(ISPICKVAL( Offer__r.Opportunity__r.Account.Customer_Compliance__c , &apos;US_Govern&apos; )  &amp;&amp;  
Name__r.NSA_Compliant__c = False  &amp;&amp;    
 (Offer__r.Opportunity__r.RecordType.DeveloperName = &apos;Indirect_Record_Type&apos; || Offer__r.Opportunity__r.RecordType.DeveloperName = &apos;Direct_Record_Type&apos;)  &amp;&amp; $Profile.Name &lt;&gt; $Label.Data_Loader_Profile_Name 
 ) , true, false)</errorConditionFormula>
    <errorMessage>New team member should be NSA compliant.</errorMessage>
</ValidationRule>
