    <fields>
        <fullName>CPQ_Bundle__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <description>This field stores the reference to the Product2 record for the Bundle. This can be either a static or dynamic bundle.</description>
        <externalId>false</externalId>
        <label>Bundle</label>
        <referenceTo>Product2</referenceTo>
        <relationshipLabel>MPW Bundles (Bundle)</relationshipLabel>
        <relationshipName>MPW_Bundles1</relationshipName>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>CPQ_Entry_Type__c</fullName>
        <description>This field is used to flag whether the record is for a static bundle or a dynamic bundle. It allows in selections to differentiate between the two types of bundles.</description>
        <externalId>false</externalId>
        <inlineHelpText>Please flag whether the record is for a static or dynamic bundle. Static bundles contain Sales Items with a defined quantity. For dynamic bundles, the quantity is calculated during runtime.</inlineHelpText>
        <label>Entry Type</label>
        <required>true</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Static</fullName>
                    <default>true</default>
                    <label>Static</label>
                </value>
                <value>
                    <fullName>Dynamic</fullName>
                    <default>false</default>
                    <label>Dynamic</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>CPQ_Main_Bundle__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <description>This field is used to store the main bundle (e.g. MPW20) of the configuration for a static bundle.</description>
        <externalId>false</externalId>
        <inlineHelpText>Please enter the main bundle of the configuration of the bundle.</inlineHelpText>
        <label>Main Bundle</label>
        <referenceTo>Product2</referenceTo>
        <relationshipLabel>MPW Bundles</relationshipLabel>
        <relationshipName>MPW_Bundles</relationshipName>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>CPQ_Quantity__c</fullName>
        <description>This field stores the quanity of a Sales Item within a bundle. This makes sense only for static bundles, as for dynamic bundles the quantities is calculated during runtime.</description>
        <externalId>false</externalId>
        <inlineHelpText>The quantity shall be provided for a bundle quantity 1.</inlineHelpText>
        <label>Quantity</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
