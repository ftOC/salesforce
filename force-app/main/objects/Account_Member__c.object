        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Account__r.Customer_Compliance__c, &apos;US_Govern&apos;), 
Name__r.NSA_Compliant__c &lt;&gt; TRUE,
NOT(OR( 
	Name__r.Profile.Name = &apos;PRM Partner Community User Login&apos;, 
	Name__r.Profile.Name = &apos;PRM Partner Relationship Manager Login&apos;))
)</errorConditionFormula>
