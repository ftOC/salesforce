    <validationRules>
        <fullName>Test_Result_Blocked</fullName>
        <active>true</active>
        <description>Added Validation Rule to field Defect: if Test Result is Blocked. User needs to select an existing Defect.</description>
        <errorConditionFormula>ISPICKVAL( Test_Result__c , &quot;Blocked&quot;) &amp;&amp; ISBLANK(Defect__c )</errorConditionFormula>
        <errorDisplayField>Defect__c</errorDisplayField>
        <errorMessage>If Test Result is Blocked, please select an existing Defect.</errorMessage>
    </validationRules>
    <validationRules>
