<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Created_after_Sprint_start__c</fullName>
    <description>Checks if req. create date is newer then release start date and returns true if newer</description>
    <externalId>false</externalId>
    <formula>if (  Sprint__r.Start_Date__c  &gt;= datevalue(CreatedDate ),
  false,
  true)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Created after Sprint start</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
