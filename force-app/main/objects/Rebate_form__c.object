    <validationRules>
        <fullName>Restrict_PSM_from_Rejection</fullName>
        <active>true</active>
        <description>D-4328</description>
        <errorConditionFormula>OR(
AND($User.ProfileId == $Label.Nokia_Sales_Profile_Id , 
 ISPICKVAL(Rebate_Form_Status__c, &apos;Rejected&apos;),  
ISPICKVAL(Pending_Rejection_Reason__c, &apos;Minimum Revenue Not Attained&apos;)),

AND($User.ProfileId == $Label.Nokia_Sales_Profile_Id , 
 ISPICKVAL(Rebate_Form_Status__c, &apos;Rejected&apos;),  
ISPICKVAL(Pending_Rejection_Reason__c, &apos;Outstanding Balance&apos;))
)</errorConditionFormula>
        <errorDisplayField>Pending_Rejection_Reason__c</errorDisplayField>
        <errorMessage>You are not allowed to select this Pending/Rejection Reason value</errorMessage>
