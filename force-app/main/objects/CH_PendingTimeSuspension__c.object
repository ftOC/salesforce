    <fields>
        <fullName>CH_CumulativeTimeMin__c</fullName>
        <description>This field holds the pending time value in minutes</description>
        <externalId>false</externalId>
        <formula>(CH_StopTime__c - CH_StartTime__c) * 24 * 60</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Cumulative time(Min)</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
