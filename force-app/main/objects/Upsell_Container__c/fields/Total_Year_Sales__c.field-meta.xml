<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Total_Year_Sales__c</fullName>
    <description>Sum of all the four quarters for a year</description>
    <externalId>false</externalId>
    <formula>Quarter1_Period_Value__c  +  Quarter2_Period_Value__c  +  Quarter3_Period_Value__c  +  Quarter4_Period_Value__c</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Total Year Sales</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
