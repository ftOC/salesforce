    <fields>
        <fullName>CH_CDB_EntitlementId__c</fullName>
        <description>Change for the integration / migration to correctly link CDB Entitlement with the CDB Service offering ( entitlement_id) against the right Salesforce Entitlement Id.
This field is also used for linking the ContractLineItem-record belonging to the 1-on-1 relationship to the Entitlement-record.</description>
        <encryptionScheme>None</encryptionScheme>
        <externalId>true</externalId>
        <label>CDB Entitlement ID</label>
        <length>255</length>
        <required>true</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>CH_EntitlementType__c</fullName>
        <defaultValue>&apos;Entitled Asset&apos;</defaultValue>
        <description>Field to determine if this is a covered asset entitlement or an entitlement for authorised contacts</description>
        <externalId>false</externalId>
        <label>Entitlement Type</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Entitled Asset</fullName>
                    <default>true</default>
                    <label>Entitled Asset</label>
                </value>
                <value>
                    <fullName>Authorized Contact</fullName>
                    <default>false</default>
                    <label>Authorized Contact</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>CH_LastFullResynchDate__c</fullName>
        <encryptionScheme>None</encryptionScheme>
        <externalId>false</externalId>
        <label>Last Full Resynch Date</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>DateTime</type>
    </fields>
    <fields>
        <fullName>CH_RecordTypeCheck__c</fullName>
        <description>To check record type for case handling project. Advantage of keeping this, whenever we have change in record type then no need to add those changes in every rule.</description>
        <externalId>false</externalId>
        <formula>RecordType.DeveloperName = &apos;CH_Entitlement&apos; || RecordType.DeveloperName = &apos;HWS_Entitlement&apos;</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Record Type Check</label>
        <trackHistory>false</trackHistory>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>CH_ServiceMappingId__c</fullName>
        <description>Name of Entitlement Process in SF which will enable mapping.</description>
        <externalId>false</externalId>
        <label>Service Mapping Id</label>
        <required>true</required>
        <trackHistory>false</trackHistory>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetName>CH_ServiceMappingType</valueSetName>
        </valueSet>
    </fields>
    <fields>
        <fullName>CH_ServiceOffering__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <description>This field is to be set by copying value from corresponding Contract Line Item</description>
        <externalId>false</externalId>
        <label>Service Offering</label>
        <referenceTo>ContractLineItem</referenceTo>
        <relationshipLabel>Entitlements</relationshipLabel>
        <relationshipName>Entitlements</relationshipName>
        <required>false</required>
        <trackHistory>true</trackHistory>
        <type>Lookup</type>
