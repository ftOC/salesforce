    <fields>
        <fullName>Entitlement_Status__c</fullName>
        <externalId>false</externalId>
        <formula>IF( ISBLANK(StartDate), &apos;Inactive&apos;, IF(TODAY() &lt;  StartDate, &apos;Inactive&apos;, IF ( ISBLANK( EndDate), &apos;Active&apos;, IF(TODAY() &lt;= EndDate, &apos;Active&apos;, &apos;Expired&apos;))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Entitlement Status</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
