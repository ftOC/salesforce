    <fields>
        <fullName>Interface_Status__c</fullName>
        <description>Specifies if the record is successfully moved to SUS external database.</description>
        <externalId>false</externalId>
        <inlineHelpText>Specifies if the record is successfully moved to SUS external database.</inlineHelpText>
        <label>Interface Status</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>true</sorted>
                <value>
                    <fullName>Completed</fullName>
                    <default>false</default>
                    <label>Completed</label>
                </value>
                <value>
                    <fullName>Error</fullName>
                    <default>false</default>
                    <label>Error</label>
                </value>
                <value>
                    <fullName>In Progress</fullName>
                    <default>false</default>
                    <label>In Progress</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>NCP_Product_Id__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
