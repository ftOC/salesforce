    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>ADMIN_AND_PRM_CAN_CANCEL_THE_ACTIVITY</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 


ISPICKVAL( Activity_Status__c , &apos;Cancelled&apos;), 
ISCHANGED(Activity_Status__c ) , 
$Permission.PRM_Co_Op_Internal_Users_Permission_Set, 
NOT( 
OR( 
$Profile.Name = &apos;System Administrator&apos;, 
$Profile.Name = &apos;Data Loader Profile&apos;, 
$Profile.Name = &apos;Nokia Admin Profile&apos;, 
$Profile.Name = &apos;Global Process Admin Profile&apos;	
) 
) 
)</errorConditionFormula>
        <errorMessage>Only Nokia Co-op Administrator and Partner Users are able to cancel Activities prior to submitting claims</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Approved_amount_not_greater_than_request</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 

Activity_Approved_Amount__c &gt; Reimbursement_Amount_Requested__c, 
NOT(AND(OnHold_Reimbursement_Amount__c &gt; 0, Reimbursement_Amount_Requested__c = 0)), 
NOT( 
OR( 
$Profile.Name = &apos;System Administrator&apos;, 
$Profile.Name = &apos;Data Loader Profile&apos;, 
$Profile.Name = &apos;Nokia Admin Profile&apos;, 
$Profile.Name = &apos;Global Process Admin Profile&apos;) 
) 
)</errorConditionFormula>
        <errorMessage>Reimbursement Approved Amount should not be greater than Reimbursement requested Amount, please correct Reimbursement Amount Approved.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_Date_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
Start_Date__c &gt; End_Date__c,

OR( 
ISCHANGED( End_Date__c ),
 ISNEW() ,
$Profile.Name = &apos;System Administrator&apos;, 
$Profile.Name = &apos;Data Loader Profile&apos;, 
$Profile.Name = &apos;Nokia Admin Profile&apos;, 
$Profile.Name = &apos;Global Process Admin Profile&apos;	
) 

)</errorConditionFormula>
        <errorDisplayField>End_Date__c</errorDisplayField>
        <errorMessage>End Date can&apos;t be Prior to Start Date. Please select a date after the Start Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Exception_Details_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
ISBLANK( Exception_Details__c ) , 
!(ISBLANK( Exception__c )), 
!(INCLUDES(Exception__c,&apos;Other&apos;)), 
NOT( 
OR(  
$Profile.Name = &apos;System Administrator&apos;, 
$Profile.Name = &apos;Data Loader Profile&apos;, 
$Profile.Name = &apos;Nokia Admin Profile&apos;, 
$Profile.Name = &apos;Global Process Admin Profile&apos;	
) 
) 
)</errorConditionFormula>
        <errorDisplayField>Exception_Details__c</errorDisplayField>
        <errorMessage>Please write the details about the exception activity that would help the Approver(s) approve the request.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Manual_Activity_Path_status_update</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
NOT(bypass_last_claim_validation__c), 
Text(Activity_Path_Status__c) != &apos;New&apos;, 
Text(Activity_Path_Status__c) != &apos;Expired/Rejected/Cancelled&apos;, 
ISCHANGED(Activity_Path_Status__c), 
( 
OR( 
$Profile.Name = &apos;PRM Partner Community User Login&apos;, 
$Profile.Name = &apos;PRM Partner Relationship Manager Login&apos;,
$Profile.Name = &apos;Customers&apos;, 
$Profile.Name = &apos;Customers Login&apos;	
) 
) 
)</errorConditionFormula>
        <errorMessage>Not Applicable</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Manual_Status_Update_Not_Allowed</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT(bypass_last_claim_validation__c),
	Text(Activity_Status__c) != &apos;New&apos;,
	Text(Activity_Status__c) != &apos;Cancelled&apos;,
	ISCHANGED(Activity_Status__c),
	( 
		OR( 
			$Profile.Name = &apos;PRM Partner Community User Login&apos;, 
			$Profile.Name = &apos;PRM Partner Relationship Manager Login&apos;	
		) 
		) 
	)</errorConditionFormula>
        <errorDisplayField>Activity_Status__c</errorDisplayField>
        <errorMessage>You are not allowed to select status other than &quot;New&quot; and &quot;Cancelled&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>On_Hold_Reason_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
On_Hold__c = TRUE,
ISBLANK(On_Hold_Reason__c),
NOT( 
OR( 
$Profile.Name = &apos;System Administrator&apos;, 
$Profile.Name = &apos;Data Loader Profile&apos;, 
$Profile.Name = &apos;Nokia Admin Profile&apos;, 
$Profile.Name = &apos;Global Process Admin Profile&apos;	
) 
) 
)</errorConditionFormula>
        <errorDisplayField>On_Hold_Reason__c</errorDisplayField>
        <errorMessage>Please give the On Hold Reason</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRM_Cancellation_Reason</fullName>
        <active>true</active>
        <description>SFPRM-282</description>
        <errorConditionFormula>AND(
				ISPICKVAL(Activity_Status__c, &apos;Cancelled&apos;),
				ISPICKVAL(PRM_Cancellation_Reason__c, &apos;&apos;)
)</errorConditionFormula>
        <errorDisplayField>PRM_Cancellation_Reason__c</errorDisplayField>
        <errorMessage>Please enter the &apos;Cancellation Reason&apos; before cancelling the activity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRM_Other_Cancellation_Reason</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
				ISPICKVAL(PRM_Cancellation_Reason__c, &apos;Other&apos;),
				ISBLANK(PRM_Other_Cancellation_Reason__c)
)</errorConditionFormula>
        <errorDisplayField>PRM_Other_Cancellation_Reason__c</errorDisplayField>
        <errorMessage>Please enter the &apos;Cancellation Reason&apos; before cancelling the activity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Requeste_amount_not_greater_than_Cost</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
Cost_of_Activity__c &lt; Reimbursement_Amount_Requested__c, 
NOT( 
OR( 
$Profile.Name = &apos;System Administrator&apos;, 
$Profile.Name = &apos;Data Loader Profile&apos;, 
$Profile.Name = &apos;Nokia Admin Profile&apos;, 
$Profile.Name = &apos;Global Process Admin Profile&apos;) 
) 
)</errorConditionFormula>
        <errorMessage>Reimbursement Amount Requested should not be greater than Cost of Activity, please correct.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_Date_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
OR(ISNEW(),
ISCHANGED(Start_Date__c )),
(Start_Date__c - TODAY() &lt;15) ,
NOT(INCLUDES( Exception__c , &apos;Activity Start Date occurred in the past&apos;)),
NOT( 
OR( 
$Profile.Name = &apos;System Administrator&apos;, 
$Profile.Name = &apos;Data Loader Profile&apos;, 
$Profile.Name = &apos;Nokia Admin Profile&apos;, 
$Profile.Name = &apos;Global Process Admin Profile&apos;	
) 
) 
)</errorConditionFormula>
        <errorMessage>Please select a date 15 days Prior to Start Date of the Activity. 
Exception: If the activity happened in past, please select the date in past and select the Exception as ‘Activity Start Date occurred in the Past’</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_manual_status_after_rejection</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
Text(Activity_Status__c) = &apos;Rejected&apos;,
ISCHANGED(Name))</errorConditionFormula>
        <errorMessage>You are not allowed to Change the Activity Details after it is rejected</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_on_Activity_cancellation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Claim_count_for_cancellation__c  &gt; 0,
ISCHANGED( Activity_Status__c ),
ISPICKVAL(Activity_Status__c ,&apos;Cancelled&apos;),
NOT( 
OR( 
$Profile.Name = &apos;System Administrator&apos;, 
$Profile.Name = &apos;Data Loader Profile&apos;, 
$Profile.Name = &apos;Nokia Admin Profile&apos;, 
$Profile.Name = &apos;Global Process Admin Profile&apos;	
) 
) 
)</errorConditionFormula>
        <errorMessage>You are not allowed to cancel the activity because one or more of the associated claims have already been submitted for Audit, passed Audit or were reimbursed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_on_Currency</fullName>
        <active>false</active>
        <errorConditionFormula>AND(ISNEW(),
(TEXT(CurrencyIsoCode) &lt;&gt;  TEXT(Fund_Id__r.CurrencyIsoCode)))</errorConditionFormula>
        <errorDisplayField>CurrencyIsoCode</errorDisplayField>
        <errorMessage>The Currency should be same as of the Account Currency. Please choose the correct currency and save the activity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_on_Other_Exception_value</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
INCLUDES( Exception__c , &apos;Other&apos;) , 
ISBLANK( If_Other_Exception_Please_Specify__c ) , 
NOT( 
OR( 
$Profile.Name = &apos;System Administrator&apos;, 
$Profile.Name = &apos;Data Loader Profile&apos;, 
$Profile.Name = &apos;Nokia Admin Profile&apos;, 
$Profile.Name = &apos;Global Process Admin Profile&apos;	
) 
) )</errorConditionFormula>
        <errorDisplayField>If_Other_Exception_Please_Specify__c</errorDisplayField>
        <errorMessage>Please write the other exception as you have selected &quot;Other&quot; in Exception field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_on_blank_activity_amount</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
ISNEW(), 
OR( 
ISBLANK(Cost_of_Activity__c), 
Cost_of_Activity__c = 0.00, Reimbursement_Amount_Requested__c = 0.00
) 
 
)</errorConditionFormula>
        <errorMessage>Please enter non zero amount in &apos;Cost of Activity&apos; and &apos;Reimbursement Amount Requested	&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_on_manual_change_after_locked</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISCHANGED( Name ),
OR(ISPICKVAL( Activity_Status__c ,&apos;Claimable&apos;),
ISPICKVAL( Activity_Status__c ,&apos;Claimed&apos;),
ISPICKVAL( Activity_Status__c ,&apos;Approved&apos;),
ISPICKVAL( Activity_Status__c ,&apos;Closed&apos;),
ISPICKVAL( Activity_Status__c ,&apos;Expired&apos;),
ISPICKVAL( Activity_Status__c ,&apos;Cancelled&apos;)))</errorConditionFormula>
        <errorMessage>You are not allowed to Change the Activity Name after it is Locked</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
