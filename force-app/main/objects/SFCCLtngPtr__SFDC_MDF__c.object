    <validationRules>
        <fullName>SFCCLtngPtr__Activity_Date_Days_In_Future</fullName>
        <active>true</active>
        <description>Confirm that the marketing activity date is a certain # weeks in the future</description>
        <errorConditionFormula>AND(   OR (      ISNEW(),       ISCHANGED( SFCCLtngPtr__Activity_Date__c )),    SFCCLtngPtr__Activity_Date__c &lt; (TODAY()+14) )</errorConditionFormula>
        <errorDisplayField>SFCCLtngPtr__Activity_Date__c</errorDisplayField>
        <errorMessage>The activity date should be two weeks or more in the future</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SFCCLtngPtr__Require_Other_Activity_Type</fullName>
        <active>true</active>
        <description>Used to force the user to choose fill in the &apos;other activity type&apos; when &apos;other&apos; is chosen as the activity type.</description>
        <errorConditionFormula>AND(ISPICKVAL( SFCCLtngPtr__Activity_Type__c, &quot;Other&quot;), LEN(SFCCLtngPtr__Other_Activity_Type__c )=0)</errorConditionFormula>
        <errorDisplayField>SFCCLtngPtr__Other_Activity_Type__c</errorDisplayField>
        <errorMessage>Please provide details on the &apos;Other&apos; activity type.</errorMessage>
    </validationRules>
