<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Market_reqd_for_UAT_April_Rel_Scripts</fullName>
    <active>true</active>
    <description>Market_reqd_for_UAT_April_Rel._Scripts</description>
    <errorConditionFormula>AND(
  ISPICKVAL(Execution_Type__c, &quot;UAT April Rel.&quot;),
  ISPICKVAL(Market__c, &quot;&quot;)
)</errorConditionFormula>
    <errorDisplayField>Market__c</errorDisplayField>
    <errorMessage>Market must be filled for UAT April Rel. test scripts</errorMessage>
</ValidationRule>
