<apex:page controller="jiffle.JNBookMeeting" standardStylesheets="false" showHeader="true" sidebar="false" tabStyle="Jifflenow_Meetings__tab">
<link rel="stylesheet" href="{!URLFOR($Resource.JNResources, 'JNResources/application.css')}"/>
<html lang="en">

<apex:include pageName="jiffle__JNConsent"/>
<apex:include pageName="jiffle__JNGlobals"/>
<div class="summit-page stick-top iris-bootstrap jiffle-book-meeting hide" id="sf-jiffle-now">

  <div class="notification-modal modal fade green" style="z-index: 1051;">
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close js-modal-close" aria-label="Close"><span aria-hidden="true">&times;</span></button>
          <h4 class="modal-title"></h4>
        </div>
        <div class="modal-body"></div>
        <div class="modal-footer">
          <button type="button" data-dismiss="modal" class="btn btn-blue modal-done" data-i18n="YES"></button>
          <button type="button" data-dismiss="modal" class="btn btn-blue modal-cancel" data-i18n="NO"></button>
        </div>
      </div>
    </div>
  </div>

  <div id='proceed-page'>
    <div class="book-meeting page-pad-top page-pad-bottom">
      <div class="container-fluid">
        <div class="row">
          <div class="col-md-12">
            <h2 class="meeting-id subnav-border" data-i18n="meeting_in_making"></h2>
            <h2 id="js_activity_name">Tracks and Sessions</h2>
            <h2 class="hide" id="js_track_name"></h2>
          </div>
        </div>

        <div class="row">
          <div class="col-md-12">
            <div class="section section-meeting-proceed">
              <div class="section-header">
                <span class="section-title js-single-meetings">
                  <span><div class="meeting-status"></div><span class='approved-text'></span></span>
                </span>
                <div class="dropdown pull-right cursor-pointer right-margin-15">
                  <a data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" class="js-secondary-action-dropdown hide">
                    <i class="jif-more"></i>
                    <div class="font-8">More</div>
                  </a>
                  <ul class="dropdown-menu js-secondary-action-list">
                    <li class="decline-meeting hide">
                      <a href="javascript:void(0)">
                        <div class="font-12" data-i18n="decline"></div>
                      </a>
                    </li>
                  </ul>
                </div>
                <div class="dropdown cancel-meeting pull-right right-margin-15 hide" style="height: 47px;">
                  <a href="javascript:void(0)" class="icon-box jif-red">
                    <i class="jif-trash left-margin-2"></i>
                    <div class="font-8" data-i18n="cancel"></div>
                  </a>
                </div>
                <ul class="menu-list">
                  <li class="approve-meeting icon-box jif-green hide">
                    <a href="javascript:void(0)">
                      <div class="font-8" data-i18n="approve"></div>
                    </a>
                  </li>
                  <li class="accept-meeting hide">
                    <a href="javascript:void(0)" class="icon-box jif-blue">
                      <i class="jif-check"></i>
                      <div class="font-8" data-i18n="accept"></div>
                    </a>
                  </li>
                </ul>
              </div>
            </div>
          </div>
        </div>

        <div class="row">
          <div class="col-md-4 col-sm-6">
            <div class="section section-meeting-proceed">
              <div class="section-content">
                <div class="row">
                  <div class="col-xs-6">
                    <div class="selected-item selected-time"></div>
                  </div>
                  <div class="col-xs-6">
                    <div class="selected-item selected-room"></div>
                  </div>
                </div>
                <div class="selected-item selected-internal-attendee no-padding-left-right">
                  <div class="selected-title" >
                    <span class="count selectedInternalAttendeeCount">0</span>
                    <span data-i18n="internal_participants"></span>
                    <div class="waitlist pull-right capitalize js-internal-waitlist-attendee-count"></div>
                  </div>
                  <div class="selected-attendee-list scroll-y">
                    <div class="attendee-empty-state internal">
                      <i class="jif-plus-circle-o jif-light-grey jif-70"></i>
                      <br/><br/><br/>
                      <a href="javascript:void(0)" class="add-participant" data-i18n="add_internal_participants"></a>
                    </div>
                  </div>
                  <div class="section-action">
                    <a href="javascript:void(0)" class='go-to-main-internal hide' data-i18n="Add"></a>
                  </div>
                </div>
                <div class="selected-item selected-external-attendee">
                  <div class="selected-title">
                    <span class="count selectedExternalAttendeeCount">0</span>
                    <span data-i18n="external_participants"></span>
                    <div class="waitlist pull-right capitalize js-external-waitlist-attendee-count"></div>
                  </div>
                  <div class="selected-attendee-list scroll-y">
                    <div class="attendee-empty-state external">
                      <i class="jif-plus-circle-o jif-light-grey jif-70"></i>
                      <br/><br/><br/>
                      <a href="javascript:void(0)" class="add-participant" data-i18n="add_external_participants"></a>
                    </div>
                  </div>
                  <div class="section-action">
                    <a href="javascript:void(0)" class='go-to-main-external hide' data-i18n="Add"></a>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="col-md-6 col-sm-6">
            <form action="javascript:void(0)" class="jiffle-form meeting-summary">
              <div class="row">
                <div class="form-group" data-form-i18n="requestor_select">
                  <div class="col-xs-10">
                    <label class="required">Requestor Name</label>
                  </div>
                  <div class="col-xs-10">
                    <select class="form-control" name="requestor_list" id='requestor-select' required="required"></select>
                    <div id="requestor-label-div" class="requestor-label-div hide"></div>
                  </div>
                </div>
              </div>
            </form>
            <hr/>
            <form action="javascript:void(0)" class="jiffle-form custom-form" id='customer_form'>
              <div class="col-md-12">
                <div class="bottom-margin-15">
                  <span class="font-22 text-uppercase" data-i18n="customer_info"></span>
                </div>
              </div>
              <div class="col-md-10">
                <div id='dynamic_form'>
                </div>
              </div>
            </form>
          </div>
        </div>
      </div>
    </div>

  </div>


  <div id="cover">
    <div class="loaders">
      <div id="circularG">
        <div id="circularG_1" class="circularG">
        </div>
        <div id="circularG_2" class="circularG">
        </div>
        <div id="circularG_3" class="circularG">
        </div>
        <div id="circularG_4" class="circularG">
        </div>
        <div id="circularG_5" class="circularG">
        </div>
        <div id="circularG_6" class="circularG">
        </div>
        <div id="circularG_7" class="circularG">
        </div>
        <div id="circularG_8" class="circularG">
        </div>
      </div>
    </div>
  </div>
</div>


<script language="javascript" type="text/javascript" src="{!URLFOR($Resource.JNResources, 'JNResources/application.js')}"></script><script language="javascript" type="text/javascript" src="{!URLFOR($Resource.JNMappingModule)}"></script>
<script type="text/javascript">
Visualforce.remoting.timeout = 120000;
</script>
</html>
</apex:page>