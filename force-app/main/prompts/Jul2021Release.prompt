<?xml version="1.0" encoding="UTF-8"?>
<Prompt xmlns="http://soap.sforce.com/2006/04/metadata">
    <masterLabel>Jul 2021 Release</masterLabel>
    <promptVersions>
        <actionButtonLabel>Launch Video</actionButtonLabel>
