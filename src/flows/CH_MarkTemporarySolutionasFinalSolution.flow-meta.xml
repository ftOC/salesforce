            <name>CH_AssignedDateIsNull</name>
            <conditionLogic>(1 OR 2 OR 3 ) AND 4 AND 5 AND 6</conditionLogic>
            <conditions>
                <leftValueReference>CH_CurrentCase.RecordTypeId</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>CH_IncidentID.Id</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>CH_CurrentCase.RecordTypeId</leftValueReference>
                <operator>EqualTo</operator>
