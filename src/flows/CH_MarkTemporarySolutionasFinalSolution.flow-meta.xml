                    <stringValue>Pending Customer</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>CH_StatusError</targetReference>
            </connector>
            <label>Status Transition</label>
        </rules>
        <rules>
            <name>CH_ODRNotCreated</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
