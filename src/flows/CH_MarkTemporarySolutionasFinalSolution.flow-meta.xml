                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Cancelled</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>CH_CurrentCase.Status</leftValueReference>
                <operator>EqualTo</operator>
