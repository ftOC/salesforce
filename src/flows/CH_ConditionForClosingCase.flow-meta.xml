    <description>System to check whether case can be closed</description>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(
	OR(
		AND( 
			[Case].IsStopped ,
			ISPICKVAL(PRIORVALUE([Case].Status ) , &quot;Pending Customer&quot;),
			ISPICKVAL([Case].Status , &quot;Closed&quot;)
			),
		AND(
			ISBLANK([Case].AccountId ),
			ISPICKVAL([Case].CH_Outage__c , &quot; &quot;) ,
			ISPICKVAL([Case].CH_CaseCause__c , &quot; &quot;),
			ISPICKVAL([Case].Status , &quot;Closed&quot;)
			)
		),
	OR(
		[Case].RecordType.DeveloperName =&quot;CH_NokiaStandardCase&quot;,
		[Case].RecordType.DeveloperName =&quot;CH_NokiaIncidentWithRestoration&quot;,
		[Case].RecordType.DeveloperName =&quot;CH_NokiaInformationRequest&quot;
		)
	
	)
</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_3</name>
        <dataType>Boolean</dataType>
        <expression>AND(
	OR(
		AND( 
			{!myVariable_current.IsStopped} ,
			ISPICKVAL(PRIORVALUE({!myVariable_current.Status} ) , &quot;Pending Customer&quot;),
			ISPICKVAL({!myVariable_current.Status} , &quot;Closed&quot;)
			),
		AND(
			ISBLANK({!myVariable_current.AccountId} ),
			ISPICKVAL({!myVariable_current.CH_Outage__c} , &quot; &quot;) ,
			ISPICKVAL({!myVariable_current.CH_CaseCause__c} , &quot; &quot;),
			ISPICKVAL({!myVariable_current.Status} , &quot;Closed&quot;)
			)
		),
	OR(
		{!myVariable_current.RecordType.DeveloperName} =&quot;CH_NokiaStandardCase&quot;,
		{!myVariable_current.RecordType.DeveloperName} =&quot;CH_NokiaIncidentWithRestoration&quot;,
		{!myVariable_current.RecordType.DeveloperName} =&quot;CH_NokiaInformationRequest&quot;
		)
	
	)</expression>
    </formulas>
