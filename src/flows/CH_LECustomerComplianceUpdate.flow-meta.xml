    <description>NOKIASC-22867 + NOKIASC-24789 + NOKIASC-24726 + NOKIASC-24755: Setting Customer Compliance for &quot;Legal Entity&quot; and &quot;Logistic Account&quot; + Creating and Updating Location Records linked to &quot;Legal Entity&quot;</description>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND ( OR
                ([Account].RecordType.DeveloperName = &apos;Legal_Entity&apos;, 
                 [Account].RecordType.DeveloperName = &apos;Customer&apos;),

           OR (
                  ISBLANK ([Account].Acc_No__c),
                  ISCHANGED ([Account].AccountNumber)
))</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
        <expression>AND ( OR
                ({!myVariable_current.RecordType.DeveloperName} = &apos;Legal_Entity&apos;, 
                 {!myVariable_current.RecordType.DeveloperName} = &apos;Customer&apos;),

           OR (
                  ISBLANK ({!myVariable_current.Acc_No__c}),
                  ISCHANGED ({!myVariable_current.AccountNumber})
))</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND (
           [Account].RecordType.DeveloperName = &quot;Legal_Entity&quot; ,
           ISCHANGED ( [Account].Customer_Compliance__c ) ,
           NOT ( ISPICKVAL ( [Account].Type , &quot;Vendor&quot; ) )
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_11</name>
        <dataType>Boolean</dataType>
        <expression>AND (
           {!myVariable_current.RecordType.DeveloperName} = &quot;Legal_Entity&quot; ,
           ISCHANGED ( {!myVariable_current.Customer_Compliance__c} ) ,
           NOT ( ISPICKVAL ( {!myVariable_current.Type} , &quot;Vendor&quot; ) )
)</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND (
           OR (
                   ISNEW() ,
                   ISCHANGED ([Account].ParentId)
           ) ,
           [Account].RecordType.DeveloperName = &quot;Legal_Entity&quot; ,
           NOT ( ISBLANK ([Account].ParentId) ) ,
           OR (
                   [Account].Parent.RecordType.DeveloperName = &quot;Customer&quot; ,
                   [Account].Parent.RecordType.DeveloperName = &quot;Prospect&quot;
           )
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_3</name>
        <dataType>Boolean</dataType>
        <expression>AND (
           OR (
                   ISNEW() ,
                   ISCHANGED ({!myVariable_current.ParentId})
           ) ,
           {!myVariable_current.RecordType.DeveloperName} = &quot;Legal_Entity&quot; ,
           NOT ( ISBLANK ({!myVariable_current.ParentId}) ) ,
           OR (
                   {!myVariable_current.Parent.RecordType.DeveloperName} = &quot;Customer&quot; ,
                   {!myVariable_current.Parent.RecordType.DeveloperName} = &quot;Prospect&quot;
           )
)</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND (
           OR (
                   ISNEW() ,
                   ISCHANGED ([Account].ParentId)
           ) ,
           [Account].RecordType.DeveloperName = &quot;Logistic_Account&quot; ,
           NOT ( ISBLANK ([Account].ParentId) ) ,
           [Account].Parent.RecordType.DeveloperName = &quot;Legal_Entity&quot;
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_5</name>
        <dataType>Boolean</dataType>
        <expression>AND (
           OR (
                   ISNEW() ,
                   ISCHANGED ({!myVariable_current.ParentId})
           ) ,
           {!myVariable_current.RecordType.DeveloperName} = &quot;Logistic_Account&quot; ,
           NOT ( ISBLANK ({!myVariable_current.ParentId}) ) ,
           {!myVariable_current.Parent.RecordType.DeveloperName} = &quot;Legal_Entity&quot;
)</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>OR (
        AND (
                   ISNEW() ,
                   [Account].RecordType.DeveloperName = &quot;Legal_Entity&quot; ,
                   NOT ( ISPICKVAL ( [Account].Type , &quot;Vendor&quot; ) ) ,
                   [Account].CH_AccountHasLocation__c = FALSE ,
                   NOT ( ISBLANK ( [Account].AccountNumber ) )
        ) ,
        AND (
                   [Account].RecordType.DeveloperName = &quot;Legal_Entity&quot; ,
                   ISCHANGED ( [Account].Type ) ,
                   ISPICKVAL ( PRIORVALUE ( [Account].Type ) , &quot;Vendor&quot; ) ,
                   [Account].CH_AccountHasLocation__c = FALSE ,
                   NOT ( ISBLANK ( [Account].AccountNumber ) )
        )
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_7</name>
        <dataType>Boolean</dataType>
        <expression>OR (
        AND (
                   ISNEW() ,
                   {!myVariable_current.RecordType.DeveloperName} = &quot;Legal_Entity&quot; ,
                   NOT ( ISPICKVAL ( {!myVariable_current.Type} , &quot;Vendor&quot; ) ) ,
                   {!myVariable_current.CH_AccountHasLocation__c} = FALSE ,
                   NOT ( ISBLANK ( {!myVariable_current.AccountNumber} ) )
        ) ,
        AND (
                   {!myVariable_current.RecordType.DeveloperName} = &quot;Legal_Entity&quot; ,
                   ISCHANGED ( {!myVariable_current.Type} ) ,
                   ISPICKVAL ( PRIORVALUE ( {!myVariable_current.Type} ) , &quot;Vendor&quot; ) ,
                   {!myVariable_current.CH_AccountHasLocation__c} = FALSE ,
                   NOT ( ISBLANK ( {!myVariable_current.AccountNumber} ) )
        )
)</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND (
           [Account].RecordType.DeveloperName = &quot;Legal_Entity&quot; ,
           ISCHANGED ( [Account].AccountNumber ) ,
           NOT ( ISPICKVAL ( [Account].Type , &quot;Vendor&quot; ) )
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_9</name>
        <dataType>Boolean</dataType>
        <expression>AND (
           {!myVariable_current.RecordType.DeveloperName} = &quot;Legal_Entity&quot; ,
           ISCHANGED ( {!myVariable_current.AccountNumber} ) ,
           NOT ( ISPICKVAL ( {!myVariable_current.Type} , &quot;Vendor&quot; ) )
)</expression>
    </formulas>
