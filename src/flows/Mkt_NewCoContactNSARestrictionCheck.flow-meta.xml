    <decisions>
        <name>Mkt_CheckforContactType</name>
        <label>Check for Contact Type</label>
        <locationX>846</locationX>
        <locationY>234</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Partner_or_Customer_Contact_Type_or_Supplier</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>$Record.CH_ContactType__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Customer</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.CH_ContactType__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Partner</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.CH_ContactType__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Supplier</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Mkt_SynctoMarketoandNSARestricted</targetReference>
            </connector>
            <label>Partner or Customer Contact Type or Supplier</label>
        </rules>
        <rules>
            <name>Mkt_SecondaryAccountContactType</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
