            </connector>
            <label>Secondary Account Contact Type</label>
        </rules>
    </decisions>
    <decisions>
        <name>Mkt_CheckforCreateorUpdateofContactRecord</name>
        <label>Check for Create or Update of Contact Record</label>
        <locationX>491</locationX>
        <locationY>213</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Mkt_ContactCreated</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Id</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Account.Customer_Compliance__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>US_Govern</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Mkt_CheckforContactType</targetReference>
            </connector>
            <label>Contact Created</label>
        </rules>
        <rules>
            <name>Mkt_ContactUpdatedandAccountChanged</name>
            <conditionLogic>3 AND 1 AND 2</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Id</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.AccountId</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>$Record__Prior.AccountId</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Account.Customer_Compliance__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>US_Govern</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Mkt_CheckforContactType</targetReference>
            </connector>
            <label>Contact Updated and Account Changed</label>
        </rules>
        <rules>
            <name>Mkt_ContactAccountChangedtoNonUsGovern</name>
            <conditionLogic>3 AND 4 AND 1 AND 2</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Account.Customer_Compliance__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>US_Govern</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record__Prior.Mkt_NSARestricted__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Id</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.AccountId</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>$Record__Prior.AccountId</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Mkt_UncheckNSARestricted</targetReference>
            </connector>
            <label>Contact Account Changed to Non Us Govern</label>
        </rules>
    </decisions>
