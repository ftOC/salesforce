                    <stringValue>Project Manager</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>previousUser</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>previousUser</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>currentUser</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>previousRoleName</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>currentRoleName</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>isDelete</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
