    <formulas>
        <description>Set the priority Value on Case Creation Based on Severity</description>
        <name>AssessedPriority</name>
        <dataType>String</dataType>
        <expression>IF( {!$Record.Id} !=null,
	TEXT({!$Record.Priority}),
	IF(
		AND(
			TEXT({!$Record.Severity__c})=&apos;Critical&apos;,
			OR(
				TEXT({!$Record.Priority})=&apos;Emergency&apos;,
				TEXT({!$Record.Priority})=&apos;Medium&apos;,
				TEXT({!$Record.Priority})=&apos;Planned&apos;,
				TEXT({!$Record.Priority})=&apos;Low&apos;
			)
		
		),	
		&apos;High&apos;,
		IF(
			TEXT({!$Record.Severity__c})=&apos;Major&apos;,
			&apos;Medium&apos;,
			IF(
				OR( TEXT({!$Record.Severity__c})=&apos;Minor&apos;, TEXT({!$Record.Severity__c})=&apos;Information Request&apos; ),
				&apos;Low&apos;,
				TEXT({!$Record.Priority})
			)
		
		)
	)
)</expression>
    </formulas>
    <formulas>
        <name>EntitlementStatusValue</name>
        <dataType>String</dataType>
        <expression>IF( TEXT({!$Record.CH_EntitlementStatus__c})==  Null,
IF( TEXT({!$Record.CH_EntitlementException__c})=  &apos;No Exception&apos;, &apos;Entitled (Automated Verification)&apos;, 
			IF( TEXT({!$Record.CH_EntitlementException__c})=  &apos;Not Applicable&apos;, &apos;Not Applicable&apos;,
				IF( TEXT({!$Record.CH_EntitlementException__c})=  &apos;Entitlement Not Listed&apos;, &apos;Pending CAPM Review&apos;, 
					IF( TEXT({!$Record.CH_EntitlementException__c})=  &apos;Entitlement Script Verified Manually&apos;, &apos;Entitled (Manually Verified)&apos;,
						IF( TEXT({!$Record.CH_EntitlementException__c})=  &apos;Entitlement Script Verification Required&apos;, &apos;Pending Manual Entitlement Script Verification&apos;,

						   
							 IF( TEXT({!$Record.CH_EntitlementException__c})=  &apos;Account Not Listed&apos;, &apos;Pending CAPM Review&apos;,
				      IF( TEXT({!$Record.CH_EntitlementException__c})=  &apos;Covered Product Asset Not Listed&apos;, &apos;Pending CAPM Review&apos;,
				         IF( TEXT({!$Record.CH_EntitlementException__c})=  &apos;Network Element Asset Not Listed&apos;, &apos;Pending CAPM Review&apos;,
				            IF( TEXT({!$Record.CH_EntitlementException__c})=  &apos;Covered Network Element Asset Not Listed&apos;, &apos;Pending CAPM Review&apos;, null
						      )
					        )
				        )
			        )
						)
					)
				)
			)
			
		),  TEXT({!$Record.CH_EntitlementStatus__c} )
)</expression>
    </formulas>
    <formulas>
        <description>Moved logic from CH_Set_Reported_Date Workflow</description>
        <name>MilestoneStartDate</name>
        <dataType>DateTime</dataType>
        <expression>IF( 
{!$Record.CH_Reported_Date__c} !=null,
{!$Record.CH_Reported_Date__c} ,
{!$Record.CH_MilestoneSLA_Start__c}
)</expression>
    </formulas>
    <formulas>
        <description>Assign Old value of Severity to SeverityOld Field</description>
        <name>SeverityOldValue</name>
        <dataType>String</dataType>
        <expression>IF( {!$Record.Id} !=null,	
		IF( TEXT({!$Record__Prior.Severity__c}) != TEXT( {!$Record.Severity__c}),
			TEXT({!$Record__Prior.Severity__c}),
			{!$Record.CH_Severity_Old__c}
		
		)
	,
		NULL
	
)</expression>
    </formulas>
    <formulas>
        <description>Assign Old Status Value</description>
        <name>StatusOldValue</name>
        <dataType>String</dataType>
        <expression>IF( {!$Record.Id} !=null,	
		IF( Text({!$Record__Prior.Status}) != Text( {!$Record.Status}),
			TEXT({!$Record__Prior.Status}),
			{!$Record.CH_Status_Old__c}
		
		)
	,
		NULL
	
)</expression>
    </formulas>
    <formulas>
        <description>Moved logic from workflow Update Customer Pending Timestamp</description>
        <name>TimeStampCustomerPending</name>
        <dataType>DateTime</dataType>
        <expression>IF(
	TExt({!$Record.status}) = &apos;Pending Customer&apos;,
	NOW(),
	{!$Record.CH_Time_Stamp_Customer_Pending__c}
)</expression>
    </formulas>
