<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Data_Quality_Last_Modified_Date_PREDICTI__c</fullName>
    <description>Req#2651: Data Quality last modified date issue for predictive analysis.</description>
    <externalId>false</externalId>
    <formula>OR( 
DQ_LastModifiedDate__c, 
(DATEVALUE($Label.Current_Period_End_Date) - DATEVALUE(Opportunity__r.Last_User_Modified__c)) &gt;= 90 
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>DQ Last Modified Date PREDICTIVE</label>
    <trackHistory>false</trackHistory>
    <type>Checkbox</type>
</CustomField>
