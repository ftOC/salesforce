<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CPQ_Test_Status__c</fullName>
    <description>aggregating the script execution line items</description>
    <externalId>false</externalId>
    <formula>IF((Total_UAT_Test_Executions_Failed__c = 0)  &amp;&amp;  ( Total_UAT_Test_Executions_Passed__c =0), &quot;Not Started&quot;,  IF(Total_UAT_Test_Executions_Failed__c &gt; 0, &quot;Failed&quot;, &quot;Passed&quot;) )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>CPQ Test Status</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
