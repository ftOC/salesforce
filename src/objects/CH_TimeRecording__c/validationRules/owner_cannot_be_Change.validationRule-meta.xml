<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>owner_cannot_be_Change</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
    NOT(
        ISNEW()),
    ISCHANGED(OwnerId)
)</errorConditionFormula>
    <errorMessage>Owner can not be changed.</errorMessage>
</ValidationRule>
