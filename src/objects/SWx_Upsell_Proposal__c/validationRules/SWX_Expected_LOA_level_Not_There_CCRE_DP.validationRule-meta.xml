<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>SWX_Expected_LOA_level_Not_There_CCRE_DP</fullName>
    <active>true</active>
    <description>Validation for before converting a CCRE DP . Expected LOA level is mandatory.</description>
    <errorConditionFormula>AND(
ISPICKVAL(Upsell_Status__c , &apos;Convert&apos;),ISPICKVAL(Analytics_Source__c ,&apos;CCRE&apos;),
ISPICKVAL(Expected_LoA_Level__c ,&apos;&apos;),$Profile.Name&lt;&gt;$Label.Data_Loader_Profile_Name)</errorConditionFormula>
    <errorMessage>Expected LOA is mandatory for every CCRE Digital Proposal.</errorMessage>
</ValidationRule>
