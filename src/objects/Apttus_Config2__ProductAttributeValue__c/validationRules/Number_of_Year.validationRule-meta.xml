<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Number_of_Year</fullName>
    <active>true</active>
    <errorConditionFormula>OR(Q_NbYearsSubsPlan__c &lt;0,Q_NbYearsSubsPlan__c &gt;3)</errorConditionFormula>
    <errorDisplayField>Q_NbYearsSubsPlan__c</errorDisplayField>
    <errorMessage>Number of yeras should be greater or equal to 0 and lower or equal to 3</errorMessage>
</ValidationRule>
