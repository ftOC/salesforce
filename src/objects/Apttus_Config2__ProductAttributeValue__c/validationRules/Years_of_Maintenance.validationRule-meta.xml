<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Years_of_Maintenance</fullName>
    <active>true</active>
    <errorConditionFormula>OR(Q_NbYearsMaintenance__c&lt;0,Q_NbYearsMaintenance__c&gt;3)</errorConditionFormula>
    <errorDisplayField>Q_NbYearsMaintenance__c</errorDisplayField>
    <errorMessage>Number of yeras of maintenance should be greater or equal to 0 and lower or equal to 3</errorMessage>
</ValidationRule>
