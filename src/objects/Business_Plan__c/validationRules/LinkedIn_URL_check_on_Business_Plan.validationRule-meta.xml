<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>LinkedIn_URL_check_on_Business_Plan</fullName>
    <active>true</active>
    <errorConditionFormula>AND(NOT( ISBLANK( LinkedIn__c ) ),NOT(
	CONTAINS(LinkedIn__c ,&apos;linkedin.com&apos;)
 )
)</errorConditionFormula>
    <errorDisplayField>LinkedIn__c</errorDisplayField>
    <errorMessage>Please enter the correct LinkedIn URL containing linkedin.com.</errorMessage>
</ValidationRule>
