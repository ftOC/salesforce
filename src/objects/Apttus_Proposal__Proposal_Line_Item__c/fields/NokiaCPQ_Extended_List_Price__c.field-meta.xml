<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>NokiaCPQ_Extended_List_Price__c</fullName>
    <description>calculates list price * quantity</description>
    <externalId>false</externalId>
    <formula>IF(ISPICKVAL( Apttus_QPConfig__ChargeType__c ,&apos;Standard Price&apos; ), Apttus_QPConfig__Quantity2__c *  Apttus_QPConfig__ListPrice__c,0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Extended List Price</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
