<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CH_Valid_Start_Threshold</fullName>
    <active>true</active>
    <errorConditionFormula>OR(
CH_Start_of_Business_Hours_Threshold__c &lt; 0, CH_Start_of_Business_Hours_Threshold__c &gt; 120
)</errorConditionFormula>
    <errorDisplayField>CH_Start_of_Business_Hours_Threshold__c</errorDisplayField>
    <errorMessage>The threshold value is out of the authorized range. the range is [0;120]</errorMessage>
</ValidationRule>
