<apex:page >
<div class="iris-bootstrap hide" id="sf-jiffle-consent">
<div class="main-page">
    <div class="vertical-middle">
        <div class="vertical-container">
            <div class="jiffle-panel">
                <div class="jiffle-panel-head">
                    <h3 data-i18n="privacy_terms_updated"></h3>
                </div>
                <div class="jiffle-panel-body">
                    <form class="jiffle-form" id="accept_consent_form" action="javascript:void(0)" accept-charset="UTF-8">
                        <div class="termsandcondition ">
                            <div class="checkbox">
                                <input type="checkbox" name="consent" id="consent" value="true" />
                                <label for="consent"></label>
                            </div>
                        </div>
                        <div class="font-11 font-dark font-black top-padding-10 bottom-padding-15" data-i18n="logged_in_opt_out_message"></div>
                        <div class="form-group hidden" data-form-i18n="token">
                            <label>Token</label>
                            <input type="hidden" id="token" name="token" value="token" />
                        </div>
                        <button type="submit" class="btn btn-lg btn-block btn-blue" data-i18n="continue"></button>
                    </form>        </div>
                    <div class="jiffle-panel-foot">
                        <div class="text-center">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
</apex:page>